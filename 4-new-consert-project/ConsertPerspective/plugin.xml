<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.eclipse.ui.perspectives" name="org.eclipse.ui.perspectives" schema="schema/org.eclipse.ui.perspectives.exsd"/>
   <extension-point id="org.eclipse.ui.perspectiveExtensions" name="org.eclipse.ui.perspectiveExtensions" schema="schema/org.eclipse.ui.perspectiveExtensions.exsd"/>
   <extension-point id="org.eclipse.ui.newWizards" name="org.eclipse.ui.newWizards" schema="schema/org.eclipse.ui.newWizards.exsd"/>
   <extension
         point="org.eclipse.ui.perspectives">
         <perspective
            class="perspective.PerspectiveFactory1"
            fixed="true"
            id="ConsertPerspective.perspective1"
            name="ConsertPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
         <perspectiveExtension
            targetID="ConsertPerspective.perspective1">
            <view
                  id="org.eclipse.jdt.ui.PackageExplorer"
                  minimized="false"
                  moveable="false"
                  ratio="0.5"
                  relationship="left"
                  relative="org.eclipse.ui.console.ConsoleView"
                  visible="true">
            </view>
 
            <view
                  id="org.eclipse.ui.console.ConsoleView"
                  minimized="false"
                  moveable="false"
                  ratio="0.5"
                  relationship="right"
                  relative="org.eclipse.jdt.ui.PackageExplorer"
                  visible="true">
            </view>
 
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="ConsertPerspective.consertProject"
            name="ConsertProject">
      </category>
      <wizard
            canFinishEarly="true"
            category="ConsertPerspective.consertProject"
            class="wizards.ConsertProjectWizard"
            finalPerspective="ConsertPerspective.perspective1"
            hasPages="false"
            icon="icons/ConsertProject.png"
            id="ConsertPerspective.ProjectWizard"
            name="ConsertProject"
            project="true">
      </wizard>
      <category
            id="ConsertPerspective.contextmodeldefinition"
            name="ContextModelDefinition">
      </category>
      <category
            id="ConsertPerspective.contextentity"
            name="ContextEntity"
            parentCategory="ConsertPerspective.contextmodeldefinition">
      </category>
      <wizard
            category="ConsertPerspective.contextmodeldefinition/ConsertPerspective.contextentity"
            class="wizards.ContextEntityWizard"
            finalPerspective="ConsertPerspective.perspective1"
            hasPages="true"
            icon="icons/cherry1600.png"
            id="ConsertPerspective.contextentity"
            name="ContextEntity"
            project="false">
      </wizard>
             
     
   </extension>

</plugin>
